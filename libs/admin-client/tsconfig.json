{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "emitDeclarationOnly": false,
    "strict": true,
    "removeComments": true,
    "declarationMap": false,
    "skipLibCheck": true
  },
  "include": ["src"]
}
